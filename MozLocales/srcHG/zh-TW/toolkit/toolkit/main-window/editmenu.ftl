### This file contains the entities needed for the 'edit' menu
### It's currently only used for the Browser Console and Developer Toolbox

editmenu-undo =
    .label = 復原
    .accesskey = U

editmenu-redo =
    .label = 取消「復原」
    .accesskey = R

editmenu-cut =
    .label = 剪下
    .accesskey = t

editmenu-copy =
    .label = 複製
    .accesskey = C

editmenu-paste =
    .label = 貼上
    .accesskey = P

editmenu-delete =
    .label = 刪除
    .accesskey = D

editmenu-select-all =
    .label = 全選
    .accesskey = A
